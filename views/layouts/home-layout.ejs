<body>
    <div id="wainiidev-page">
        <a href="#" class="js-wainiidev-nav-toggle wainiidev-nav-toggle"><i></i></a>
        <%- include('../../templates/c_nav-bar')%>
            <div id="wainiidev-main">
                <% if (fullLayout) { %>
                    <% let clsLeft=isHideSidebar ? 'col-xl-12' : 'col-xl-8' ; let clsRight=isHideSidebar ? 'd-none'
                        : 'col-xl-4' ; %>
                        <section class="ftco-section ftco-no-pt ftco-no-pb">
                            <div class="container-fluid">
                                <div class="row d-flex">
                                    <div class="<%= clsLeft %> col-md-12 py-5 px-md-5">
                                        <%- body %>
                                    </div>
                                    <div class="<%= clsRight %> sidebar ftco-animate bg-light pt-5">
                                        <div class="sidebar-box pt-md-4">
                                            <form method="post" action="" class="search-form js-search-form">
                                                <div class="form-group">
                                                    <span class="icon icon-search"></span>
                                                    <input type="text" class="form-control js-search-input" name="title"
                                                        placeholder="Type a keyword and hit enter" />
                                                </div>
                                            </form>
                                        </div>
                                        <%- include('../../components/c_categories')%>
                                            <!-- <%- include('../../components/c_popular-articles')%> -->
                                            <%- include('../../components/c_tag-cloud')%>
                                                <%- include('../../components/c_newsletter')%>
                                                    <%- include('../../components/c_statistical')%>
                                                        <!-- <%- include('../../components/c_archives')%> -->
                                    </div>
                                </div>
                            </div>
                        </section>
                        <% } else { %>
                            <%- body %>
                                <% } %>
            </div>
    </div>

    <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px">
            <circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" />
            <circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10"
                stroke="#F96D00" />
        </svg></div>

    <script src="/js/plugin/jquery.min.js"></script>
    <script src="/js/plugin/jquery-migrate-3.0.1.min.js"></script>
    <script src="/js/plugin/popper.min.js"></script>
    <script src="/js/plugin/bootstrap.min.js"></script>
    <script src="/js/plugin/jquery.easing.1.3.js"></script>
    <script src="/js/plugin/jquery.waypoints.min.js"></script>
    <script src="/js/plugin/jquery.stellar.min.js"></script>
    <script src="/js/plugin/owl.carousel.min.js"></script>
    <script src="/js/plugin/jquery.magnific-popup.min.js"></script>
    <script src="/js/plugin/aos.js"></script>
    <script src="/js/plugin/jquery.animateNumber.min.js"></script>
    <script src="/js/plugin/scrollax.min.js"></script>

    <script type="text/javascript" src="//cdn.jsdelivr.net/gh/kenwheeler/slick@1.8.1/slick/slick.min.js"></script>
    <link href="https://cdn.jsdelivr.net/npm/sweetalert2@11/dist/sweetalert2.min.css" rel="stylesheet" />
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

    <script type="module" src="/js/main.js"></script>
    <script type="module" src="/js/app.js"></script>
</body>

</html>